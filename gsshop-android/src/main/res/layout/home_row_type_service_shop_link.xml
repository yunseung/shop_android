<?xml version="1.0" encoding="UTF-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="10dp"
    android:background="@color/white"
    xmlns:card="http://schemas.android.com/apk/res-auto"
    >

    <View
        android:id="@+id/back_top"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="#adcf00"
        android:visibility="gone"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <gsshop.mobile.v2.home.shop.renewal.views.CommonTitleLayout
            android:id="@+id/common_title_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <LinearLayout
            android:id="@+id/service_type_1"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:orientation="horizontal"
            android:visibility="visible">

            <View
                android:id="@+id/space_1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="visible" />

            <RelativeLayout
                android:id="@+id/service_1"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/img_service_1"
                    android:layout_width="56dp"
                    android:layout_height="56dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="12dp" />

                <TextView
                    android:id="@+id/txt_service_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/img_service_1"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="8dp"
                    android:ellipsize="none"
                    android:paddingLeft="4dp"
                    android:paddingRight="4dp"
                    android:singleLine="true"
                    android:textColor="#111111"
                    android:textSize="13sp" />

            </RelativeLayout>

            <View
                android:id="@+id/space_2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="gone" />

            <RelativeLayout
                android:id="@+id/service_2"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/img_service_2"
                    android:layout_width="56dp"
                    android:layout_height="56dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="12dp" />

                <TextView
                    android:id="@+id/txt_service_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/img_service_2"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="8dp"
                    android:ellipsize="none"
                    android:paddingLeft="4dp"
                    android:paddingRight="4dp"
                    android:singleLine="true"
                    android:textColor="#111111"
                    android:textSize="13sp" />

            </RelativeLayout>

            <View
                android:id="@+id/space_3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="gone" />

            <RelativeLayout
                android:id="@+id/service_3"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/img_service_3"
                    android:layout_width="56dp"
                    android:layout_height="56dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="12dp" />

                <TextView
                    android:id="@+id/txt_service_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/img_service_3"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="8dp"
                    android:ellipsize="none"
                    android:paddingLeft="4dp"
                    android:paddingRight="4dp"
                    android:singleLine="true"
                    android:textColor="#111111"
                    android:textSize="13sp" />

            </RelativeLayout>

            <View
                android:id="@+id/space_4"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="gone" />

            <RelativeLayout
                android:id="@+id/service_4"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/img_service_4"
                    android:layout_width="56dp"
                    android:layout_height="56dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="12dp" />

                <TextView
                    android:id="@+id/txt_service_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/img_service_4"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="8dp"
                    android:ellipsize="none"
                    android:paddingLeft="4dp"
                    android:paddingRight="4dp"
                    android:singleLine="true"
                    android:textColor="#111111"
                    android:textSize="13sp" />

            </RelativeLayout>

            <View
                android:id="@+id/space_5"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="gone" />

            <RelativeLayout
                android:id="@+id/service_5"
                android:layout_width="64dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/img_service_5"
                    android:layout_width="56dp"
                    android:layout_height="56dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="12dp" />

                <TextView
                    android:id="@+id/txt_service_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/img_service_5"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="8dp"
                    android:ellipsize="none"
                    android:paddingLeft="4dp"
                    android:paddingRight="4dp"
                    android:singleLine="true"
                    android:textColor="#111111"
                    android:textSize="13sp" />

            </RelativeLayout>

            <View
                android:id="@+id/space_6"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:visibility="gone" />


        </LinearLayout>



        <LinearLayout
            android:id="@+id/service_type_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="gone">

            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="7.5dp"
                android:layout_marginBottom="12dp"
                card:cardCornerRadius="8dp"
                card:cardElevation="2dp"
                >
                <LinearLayout
                android:id="@+id/service_6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center_horizontal"
                >

                <ImageView
                    android:id="@+id/img_service_6"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="10dp"
                    />
                <TextView
                    android:id="@+id/txt_type2_1st_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:ellipsize="none"
                    android:singleLine="true"
                    android:textColor="#111111"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/txt_type2_1st_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="none"
                    android:singleLine="true"
                    android:textColor="#111111"
                    android:textSize="16sp"
                    android:layout_marginBottom="12dp"
                    />

            </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="7.5dp"
                android:layout_marginRight="16dp"
                android:layout_marginBottom="12dp"
                card:cardCornerRadius="8dp"
                card:cardElevation="2dp"
                >
                <LinearLayout
                    android:id="@+id/service_7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center_horizontal">

                <ImageView
                    android:id="@+id/img_service_7"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="10dp"
                    />
                <TextView
                    android:id="@+id/txt_type2_2nd_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:ellipsize="none"
                    android:singleLine="true"
                    android:textColor="#111111"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/txt_type2_2nd_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="none"
                    android:singleLine="true"
                    android:textColor="#111111"
                    android:textSize="16sp"
                    android:layout_marginBottom="12dp"
                    />

            </LinearLayout>
            </androidx.cardview.widget.CardView>

        </LinearLayout>
    </LinearLayout>

</FrameLayout>