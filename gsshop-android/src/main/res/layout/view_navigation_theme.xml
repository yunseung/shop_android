<?xml version="1.0" encoding="UTF-8"?>
<gsshop.mobile.v2.menu.navigation.ThemeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/white"
    >
    <TextView
        android:id="@+id/txt_navi_theme_title"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:paddingLeft="20dp"
        android:paddingRight="40dp"
        android:gravity="left|bottom"
        android:paddingBottom="8dp"
        android:text="@string/navi_title_partner"
        android:lines="1"
        android:textSize="15dp"
        android:textStyle="bold"
        android:textColor="#111111"
        />

    <gsshop.mobile.v2.library.viewpager.InfiniteViewPager
        android:id="@+id/viewPager_theme"
        android:layout_width="match_parent"
        android:layout_height="348dp"
        android:paddingBottom="6dp"
        android:paddingTop="2dp"
        />

    <LinearLayout
        android:id="@+id/layout_page_img"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_gravity="center"/>

</gsshop.mobile.v2.menu.navigation.ThemeLayout>