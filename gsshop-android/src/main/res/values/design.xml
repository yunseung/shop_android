    <!-- 상품평 작성 화면의 별점 모양 -->
    <!-- 별 이미지 자체가 좌우 여백을 가지고 있으므로  디자인 가이드의 marginRight값에서 여백만큼을 빼준다 (22.25dip-5.62dp=16.88dip)-->
    <style name="ReviewRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:layout_marginLeft">16.88dip</item>
        <item name="android:progressDrawable">@drawable/custom_ratingbar_full</item>
        <item name="android:minHeight">24.75dip</item>
        <item name="android:maxHeight">24.75dip</item>
        <item name="android:numStars">5</item>
        <item name="android:rating">0</item>
        <item name="android:stepSize">1</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
    </style>

    <!-- 단품 상품 이름 영역의 별점 모양 -->
    <!-- 별 이미지 자체가 좌 여백을 가지고 있으므로 여백만큼을 빼준다 (-5.62dp)-->
    <style name="ReviewRatingBarForProduct" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_star_product_detail_full</item>
        <item name="android:layout_marginLeft">-0.5dip</item>
        <item name="android:minHeight">14dip</item>
        <item name="android:maxHeight">14dip</item>
        <item name="android:numStars">5</item>
        <item name="android:rating">0</item>
